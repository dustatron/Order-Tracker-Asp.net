<form action="/vender/@Model.Id/orders/create" method="post">
  <div class="form-group">
    <label for="productName">Product Name</label>
    <input type="text" name="productName" class="form-control";>
  </div>
  
  <div class="form-group">
    <label for="quantity">Quantity</label>
    <input type="text" name="quantity" class="form-control">
  </div>

  <div class="form-group">
    <label for="deliverBy">Deliver By</label>
    <input type="text" name="deliverBy" class="form-control">
  </div>

  <input type="hidden" name="VenderId" id="@Model.Id">
  <input type="hidden" name="venderName" id="@Model.VenderName">
  <button type="submit" class="btn btn-primary btn-block">Add</button>
</form>